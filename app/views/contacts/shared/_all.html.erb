<%if current_user.contacts.present? %>
<div class="card">
<div class="table-responsive">
<table class="table table-striped  table-hover">
  <tbody>
    <% @contacts.each do |contact| %>
    <%if current_user && contact.user == current_user%>

      <tr>

        <td>
        <%if contact.image? %>
        <%= image_tag(contact.image_url ,class:"avatar-2" ) if contact.image?  %>
        <%else%>
        <%=image_tag("img_avatar.png", class:"avatar-2")%>
        <%end%>
        </td>
        <td style="width:50%"><span><%= contact.first_name %> <%= contact.last_name %></span><br>
          <small class="text-muted">
            <div data-v-9de6ee4e="" class="ProfileTag"><div data-v-9de6ee4e="" class="ProfileTag__Icon" style="background-color: #6c757d;"><svg fill="white" height="24" viewBox="0 0 24 24" width="24" xmlns="http://www.w3.org/2000/svg">
              <path d="M15.573,11.624c0.568-0.478,0.947-1.219,0.947-2.019c0-1.37-1.108-2.569-2.371-2.569s-2.371,1.2-2.371,2.569c0,0.8,0.379,1.542,0.946,2.019c-0.253,0.089-0.496,0.2-0.728,0.332c-0.743-0.898-1.745-1.573-2.891-1.911c0.877-0.61,1.486-1.666,1.486-2.812c0-1.79-1.479-3.359-3.162-3.359S4.269,5.443,4.269,7.233c0,1.146,0.608,2.202,1.486,2.812c-2.454,0.725-4.252,2.998-4.252,5.685c0,0.218,0.178,0.396,0.395,0.396h16.203c0.218,0,0.396-0.178,0.396-0.396C18.497,13.831,17.273,12.216,15.573,11.624 M12.568,9.605c0-0.822,0.689-1.779,1.581-1.779s1.58,0.957,1.58,1.779s-0.688,1.779-1.58,1.779S12.568,10.427,12.568,9.605 M5.06,7.233c0-1.213,1.014-2.569,2.371-2.569c1.358,0,2.371,1.355,2.371,2.569S8.789,9.802,7.431,9.802C6.073,9.802,5.06,8.447,5.06,7.233 M2.309,15.335c0.202-2.649,2.423-4.742,5.122-4.742s4.921,2.093,5.122,4.742H2.309z M13.346,15.335c-0.067-0.997-0.382-1.928-0.882-2.732c0.502-0.271,1.075-0.429,1.686-0.429c1.828,0,3.338,1.385,3.535,3.161H13.346z"></path>
            </svg></div>
            <div data-v-9de6ee4e="" class="ProfileTag__Name"><%=contact.group%></div>
            </div>
          </small>
        </td>
        <td style="width:100%">+<%= contact.phone %></td>
        <td >
          <div class="btn-group" style="margin-top: 20px;">
          <%= link_to contact,  class:"btn btn-sm btn-outline-secondary"  do %>
            <i class="mdi mdi-view-list" data-toggle="tooltip" title="Show" > </i>
        <%end%>
        <%= link_to contact, method: :delete, data: { confirm: 'Are you sure?' } , class:"btn btn-sm btn-outline-secondary" do %>
            <i class=" fa fa-trash-o" data-toggle="tooltip" title="Delete" ></i>
        <%end%>
        </div>
        </td>

      </tr>
      <%end%>
    <%end%>
  </tbody>
</table>
</div>
</div>
<%else%>
<%=render'contacts/shared/error'%>
<%end%>
